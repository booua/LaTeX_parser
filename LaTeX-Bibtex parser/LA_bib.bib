@Article{wilcox1197965,
  Title                    = {A rapid signal processing technique to remove the effect of dispersion from guided wave signals},
  Author                   = {Wilcox, P.D.},
  Journal                  = {Ultrasonics, Ferroelectrics and Frequency Control, IEEE Transactions on},
  Year                     = {2003},

